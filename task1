from random import seed
from random import randint
import numpy as np
#1task

print("task 1")

A = np.zeros(10)
A[4] = 1
print (A)

#2-3task

print("task 2-3")

B = np.arange(10,50)
print (B)
B[0] = B[-1] + B[0]
B[-1] = B[0] - B[-1]
B[0] = B[0] - B[-1]
print(B)

#4task

print("task 4")

C = np.arange(0,9)
C = C.reshape(3,3)
print (C)

