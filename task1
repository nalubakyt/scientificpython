#10task

print("task 10")

A = np.zeros((8,8),dtype=int)
A[1::2,::2] = 1
A[::2,1::2] = 1
print(A)

#11task
print("task 11")
V = np.random.random((5,5))
a,b = V.max(), V.min()
V = (V-a)/(b-a)
print(V)



#14task
print("task 14")
V = np.zeros((5,5))
V += np.arange(5)
print(V)

#15task
print("task15")
A = np.linspace(0,1,12,endpoint=True)[1:-1]
